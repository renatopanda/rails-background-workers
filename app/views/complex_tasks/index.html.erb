<p id="notice"><%= notice %></p>

<h1>Complex Tasks</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Result</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @complex_tasks.each do |complex_task| %>
      <tr>
        <td><%= complex_task.title %></td>
        <td><%= complex_task.result %></td>
        <td><%= link_to 'Show', complex_task %></td>
        <td><%= link_to 'Edit', edit_complex_task_path(complex_task) %></td>
        <td><%= link_to 'Destroy', complex_task, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Complex Task', new_complex_task_path %>
